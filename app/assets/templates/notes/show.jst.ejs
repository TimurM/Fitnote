<li class="note-heading" data-note-id="<%= note.id %>">
    <div class="note-preview-title"><%= note.escape('title') %>
    </div>

    <div class="note-preview-body">
    <span class="note-preview-time">
        <%= moment(note.escape("created_at")).fromNow() %>
      </span>
    <%= note.escape('body') %>
    </div>
</li>
